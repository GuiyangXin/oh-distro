package drc;

struct map_delta_t
{
  // timestamp of this message
  int64_t utime;

  // id of this request
  int64_t message_id;

  // id of map to be updated
  int32_t map_id;

  // array of points to be added
  int32_t n_added;
  vector_3d_t added[n_added];

  // array of points to be removed
  int32_t n_removed;
  vector_3d_t removed[n_removed];
}
