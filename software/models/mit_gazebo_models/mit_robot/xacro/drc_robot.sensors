<robot name="drc_skeleton">
  <!-- Spinning Hokuyo Laser -->
  <joint name="head_hokuyo_joint" type="continuous">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="1 0 0" />
    <parent link="head"/>
    <child link="head_hokuyo"/>
    <limit effort="5" velocity="12"/>
    <safety_controller k_position="100.0" k_velocity="100.0" soft_lower_limit="-9.2146" soft_upper_limit="10.7854"/>
    <dynamics damping="0.1" friction="0"/>
  </joint>
  <link name="head_hokuyo" type="laser">
    <inertial>
      <mass value="0.1" />
      <origin xyz="0 0 0" rpy="0 0 0" />
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
    </inertial>
    <visual>
      <origin rpy="0 -0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://mit_robot/meshes/head.dae" scale="1 1 1"/>
      </geometry>
    </visual>
  </link>
  <gazebo reference="head_hokuyo">
    <sensor type="ray" name="head_hokuyo_sensor">
      <pose>0 0 0 0 0 0</pose>
      <visualize>false</visualize>
      <update_rate>40</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>640</samples>
            <resolution>1</resolution>
            <min_angle>-1.57</min_angle>
            <max_angle>1.57</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.08</min>
          <max>10.0</max>
          <resolution>0.01</resolution>
        </range>
      </ray>
      <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
        <gaussianNoise>0.005</gaussianNoise>
        <alwaysOn>true</alwaysOn>
        <updateRate>40</updateRate>
        <topicName>/rotating_scan</topicName>
        <frameName>head_hokuyo</frameName>
      </plugin>
    </sensor>
  </gazebo>
<!--
  <transmission name="head_hokuyo_joint_trans" type="pr2_mechanism_model/SimpleTransmission">
    <actuator name="head_hokuyo_joint_motor"/>
    <joint name="head_hokuyo_joint"/>
    <mechanicalReduction>1</mechanicalReduction>
  </transmission> -->



  <!-- Re-righting Hokuyo Laser -->
  <joint name="head_rightedhokuyo_joint" type="continuous">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0" />
    <parent link="head"/>
    <child link="head_rightedhokuyo"/>
    <limit effort="5" velocity="12"/>
    <safety_controller k_position="100.0" k_velocity="100.0" soft_lower_limit="-9.2146" soft_upper_limit="10.7854"/>
    <dynamics damping="0.1" friction="0"/>
  </joint>
  <link name="head_rightedhokuyo" type="laser">
    <inertial>
      <mass value="0.1" />
      <origin xyz="0 0 0" rpy="0 0 0" />
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1" />
    </inertial>
    <visual>
      <origin rpy="0 -0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://mit_robot/meshes/head.dae" scale="2 2 2"/>
      </geometry>
    </visual>
  </link>
  <gazebo reference="head_rightedhokuyo">
    <sensor type="ray" name="head_rightedhokuyo_sensor">
      <pose>0 0 0 0 0 0</pose>
      <visualize>false</visualize>
      <update_rate>40</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>1440</samples>
            <resolution>1</resolution>
            <min_angle>-3.14</min_angle>
            <max_angle>3.14</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.08</min>
          <max>10.0</max>
          <resolution>0.01</resolution>
        </range>
      </ray>
      <plugin name="gazebo_ros_rightedhead_hokuyo_controller" filename="libgazebo_ros_laser.so">
        <gaussianNoise>0.005</gaussianNoise>
        <alwaysOn>true</alwaysOn>
        <updateRate>40</updateRate>
        <topicName>/righted_scan</topicName>
        <frameName>head_rightedhokuyo</frameName>
      </plugin>
    </sensor>
  </gazebo>
<!--
  <transmission name="head_rightedhokuyo_joint_trans" type="pr2_mechanism_model/SimpleTransmission">
    <actuator name="head_rightedhokuyo_joint_motor"/>
    <joint name="head_rightedhokuyo_joint"/>
    <mechanicalReduction>1</mechanicalReduction>
  </transmission> -->



  <!-- Static Hokuyo Laser -->
  <joint name="head_statichokuyo_joint" type="fixed">
     <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
<!--    <origin xyz="0 0 0" rpy="0.0 3.142 3.142"/> -->
    <axis xyz="1 0 0" />
    <parent link="head"/>
    <child link="head_statichokuyo"/>
    <limit effort="5" lower="-3.142" upper="3.142" velocity="12"/>
    <safety_controller k_position="100.0" k_velocity="100.0" soft_lower_limit="-9.2146" soft_upper_limit="10.7854"/>
    <dynamics damping="0.1" friction="0"/>
  </joint>
  <link name="head_statichokuyo" type="laser">
    <inertial>
      <mass value="0.1" />
      <origin xyz="0 0 0" rpy="0 0 0" />
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001" />
    </inertial>
<!--    <visual>
      <origin rpy="0 -0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://mit_robot/meshes/head.dae" scale="1 1 1"/>
      </geometry> 
    </visual>-->
  </link>
  <gazebo reference="head_statichokuyo">
    <sensor type="ray" name="head_statichokuyo_sensor">
      <pose>0 0 0 0 0 0</pose>
      <visualize>false</visualize>
      <update_rate>40</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>640</samples>
            <resolution>1</resolution>
            <min_angle>-1.57</min_angle>
            <max_angle>1.57</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.08</min>
          <max>10.0</max>
          <resolution>0.01</resolution>
        </range>
      </ray>
      <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
        <gaussianNoise>0.005</gaussianNoise>
        <alwaysOn>true</alwaysOn>
        <updateRate>40</updateRate>
        <topicName>/scan</topicName>
        <frameName>head_statichokuyo</frameName>
      </plugin>
    </sensor>
  </gazebo>

  <!-- head imu -->
  <link name="head_imu_link">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.01"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>
  <joint name="head_imu_joint" type="fixed">
    <parent link="head"/>
    <child link="head_imu_link"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>
  <gazebo>
    <plugin filename="libgazebo_ros_imu.so" name="drc_robot_imu_head">
      <alwaysOn>true</alwaysOn>
      <updateRate>100.0</updateRate>
      <bodyName>head_imu_link</bodyName>
      <topicName>head_imu</topicName>
      <gaussianNoise>0.0</gaussianNoise>
      <xyzOffsets>0 0 0</xyzOffsets>
      <rpyOffsets>0 0 0</rpyOffsets>
    </plugin>
  </gazebo>  
  
  

  <!-- Stereo Camera -->
  <joint name="left_camera_frame_joint" type="fixed">
<!--was:    <origin xyz="0.03 0.1 0.1" /> -->
    <origin xyz="0.03 0.35 0.1" />
    <parent link="head"/>
    <child link="left_camera_frame"/>
  </joint>
  <link name="left_camera_frame">
    <inertial>
      <mass value="0.1" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.001"  ixy="0.0"  ixz="0.0" iyy="0.001"  iyz="0.0" izz="0.001" />
    </inertial>
  </link>
  <joint name="left_camera_optical_frame_joint" type="fixed">
    <origin xyz="0 0 0" rpy="-1.5708 0.0 -1.5708" />
    <parent link="left_camera_frame" />
    <child link="left_camera_optical_frame"/>
  </joint>
  <link name="left_camera_optical_frame"/>
  <gazebo reference="left_camera_frame">
    <sensor type="camera" name="left_camera_sensor">
      <update_rate>25.0</update_rate>
      <camera>
        <horizontal_fov>1.5708</horizontal_fov>
        <image>
          <width>320</width>
          <height>240</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.01</near>
          <far>100</far>
        </clip>
      </camera>
      <plugin name="left_camera_controller" filename="libgazebo_ros_camera.so">
        <alwaysOn>true</alwaysOn>
        <updateRate>25.0</updateRate>
        <cameraName>left_eye</cameraName>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>
        <frameName>left_camera_optical_frame</frameName>
        <hackBaseline>0.0</hackBaseline>
        <distortionK1>0.0</distortionK1>
        <distortionK2>0.0</distortionK2>
        <distortionK3>0.0</distortionK3>
        <distortionT1>0.0</distortionT1>
        <distortionT2>0.0</distortionT2>
      </plugin>
    </sensor>
  </gazebo>


  <joint name="right_camera_frame_joint" type="fixed">
<!--was:    <origin xyz="0.03 -0.1 0.1" /> -->
    <origin xyz="0.03 -0.35 0.1" />
    <parent link="head"/>
    <child link="right_camera_frame"/>
  </joint>
  <link name="right_camera_frame">
    <inertial>
      <mass value="0.1" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.001"  ixy="0.0"  ixz="0.0" iyy="0.001"  iyz="0.0" izz="0.001" />
    </inertial>
  </link>
  <joint name="right_camera_optical_frame_joint" type="fixed">
    <origin xyz="0 0 0" rpy="-1.5708 0.0 -1.5708" />
    <parent link="right_camera_frame" />
    <child link="right_camera_optical_frame"/>
  </joint>
  <link name="right_camera_optical_frame"/>
  <gazebo reference="right_camera_frame">
    <sensor type="camera" name="right_camera_sensor">
      <update_rate>25.0</update_rate>
      <camera>
        <horizontal_fov>1.5708</horizontal_fov>
        <image>
          <width>320</width>
          <height>240</height>
          <format>R8G8B8</format>
        </image>
        <clip>
          <near>0.01</near>
          <far>100</far>
        </clip>
      </camera>
      <plugin name="right_camera_controller" filename="libgazebo_ros_camera.so">
        <alwaysOn>true</alwaysOn>
        <updateRate>25.0</updateRate>
        <cameraName>right_eye</cameraName>
        <imageTopicName>image_raw</imageTopicName>
        <cameraInfoTopicName>camera_info</cameraInfoTopicName>
        <frameName>right_camera_optical_frame</frameName>
<!-- was:        <hackBaseline>-0.2</hackBaseline> --><!-- check this -->
        <hackBaseline>-0.7</hackBaseline> <!-- check this -->
        <distortionK1>0.0</distortionK1>
        <distortionK2>0.0</distortionK2>
        <distortionK3>0.0</distortionK3>
        <distortionT1>0.0</distortionT1>
        <distortionT2>0.0</distortionT2>
      </plugin>
    </sensor>
  </gazebo>
</robot>

