<?xml version="1.0" ?>
<launch>
  <!-- Start the arm testbed launcher -->
  <arg name="ui_file" default="$(find V1)/ui/neck.ui
                              $(find V1)/ui/trunk.ui
                              $(find V1)/ui/arm.ui
                              $(find V1)/ui/arm.ui
                              $(find V1)/ui/leg.ui
                              $(find V1)/ui/leg.ui" />

  <arg name="tabnames" default="Neck Trunk 'Left Arm' 'Right Arm' 'Left Leg' 'Right Leg' "/>
  <arg name="lacoeff5" default="$(find drc_coeffs)/V1A/LeftArm/v1_aj5_03.xml"/>
  <arg name="racoeff5" default="$(find drc_coeffs)/V1A/RightArm/v1_aj5_02.xml"/>

  <arg name="neck_params" default="'j1NodePath=/neck/j1 j2NodePath=/neck/j2 j3NodePath=/neck/j3'"/>
  <arg name="trunk_params" default="'j1NodePath=/pelvis/wj1 j2NodePath=/trunk/wj2 j3NodePath=/trunk/wj3 limb=Trunk'"/>

  <arg name="left_arm_params" default="'j1NodePath=/trunk/laj1 j2NodePath=/left_arm/j2 j3NodePath=/left_arm/j3 j4NodePath=/left_arm/j4 j5NodePath=/left_arm/j5 j6NodePath=/left_arm/forearm_driver1/wrist_right j7NodePath=/left_arm/forearm_driver1/wrist_left side=Left limb=LeftArm channel=left_arm node=j5 coeff=$(arg lacoeff5)'"/>

  <arg name="right_arm_params" default="'j1NodePath=/trunk/raj1 j2NodePath=/right_arm/j2 j3NodePath=/right_arm/j3 j4NodePath=/right_arm/j4 j5NodePath=/right_arm/j5 j6NodePath=/right_arm/forearm_driver1/wrist_right j7NodePath=/right_arm/forearm_driver1/wrist_left side=Right limb=RightArm channel=right_arm node=j5 coeff=$(arg racoeff5)'"/>

  <arg name="left_leg_params" default="'j1NodePath=/pelvis/lj1 j2NodePath=/left_leg/j2 j3NodePath=/left_leg/j3 j4NodePath=/left_leg/j4 j5NodePath=/left_leg/j5 j6NodePath=/left_leg/j6 side=Left limb=LeftLeg'"/>

  <arg name="right_leg_params" default="'j1NodePath=/pelvis/rj1 j2NodePath=/right_leg/j2 j3NodePath=/right_leg/j3 j4NodePath=/right_leg/j4 j5NodePath=/right_leg/j5 j6NodePath=/right_leg/j6 side=Right limb=RightLeg'"/>

  <arg name="params" value="$(arg neck_params) $(arg trunk_params) $(arg left_arm_params) $(arg right_arm_params) $(arg left_leg_params) $(arg right_leg_params)"/>
  <arg name="title" default="'Full UI'"/>
  <arg name="position" default="10,10"/>
  <arg name="size" default="0,0"/>
  <node name="full_ui" pkg="ui_builder" type="ui_builder.py" args="-u $(arg ui_file)
  																		--tabnames $(arg tabnames)
                                      -t $(arg title)
                                      --position $(arg position)
                                      --size $(arg size)
  																		--parameters $(arg params)
  																		--" output="screen"/>
</launch>