            <row label="J6">
                <object name="utils" method="getString" args="NA" argTypes="str"/>
                <object name="utils" method="getString" args="NA" argTypes="str"/>
                <object name="j6" method="getModeState" thresholds="undefined,disabled,enabled" colors="gray,gray,green,yellow"/>
