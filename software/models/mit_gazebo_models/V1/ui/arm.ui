        <widget row="1" col="2" name="Joint Data" type="TableViewDisplay">
            <header value="position, velocity, effort, posCom, velCom, effCom"/>
            <row label="J1-ShoulderExtensor">
                <object name="ShoulderExtensor" method="getPosition"/>
                <object name="ShoulderExtensor" method="getVelocity"/>
                <object name="ShoulderExtensor" method="getEffort"/>
