            <row label="J7">
                <object name="utils" method="getString" args="NA" argTypes="str"/>
                <object name="utils" method="getString" args="NA" argTypes="str"/>
                <object name="j7" method="getModeState" thresholds="undefined,disabled,enabled" colors="gray,gray,green,yellow"/>
                <object name="utils" method="getString" args="NA" argTypes="str"/>
                <object name="utils" method="getString" args="NA" argTypes="str"/>
