                    <object name="ui_utils" method="systemCall" args="rosrun V1 manage_cyberdiscs.sh -b cyberdisc-1.local shutdown" argTypes="str"/>
                </widget>
                <widget name="Reboot" type="PushButton" color="yellow" textColor="black">
                    <object name="ui_utils" method="systemCall" args="rosrun V1 manage_cyberdiscs.sh -b cyberdisc-1.local reboot" argTypes="str"/>
