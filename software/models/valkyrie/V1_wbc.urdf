    </inertial>
  </link>
  <gazebo reference="/v1/LeftLegHokuyo_Frame">
    <sensor name="/v1/LeftLegHokuyo" type="ray">
      <pose>0 0 0 0 0 0</pose>
      <visualize>false</visualize>
      <update_rate>40.0</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>1080</samples>
            <resolution>0.0043611</resolution>
            <min_angle>-2.355</min_angle>
            <max_angle>2.355</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.1</min>
          <max>30</max>
          <resolution>0.03</resolution>
        </range>
        <!-- Noise data from DRCSIM -->
        <noise>
          <type>gaussian</type>
          <!-- Noise parameters based on published spec for Hokuyo laser
               achieving "+-30mm" accuracy at range < 10m.  A mean of 0.0m and
               stddev of 0.01m will put 99.7% of samples within 0.03m of the true
               reading. -->
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin filename="libgazebo_ros_laser.so" name="/v1/LeftLegHokuyo_Controller">
        <gaussianNoise>0.005</gaussianNoise>
        <alwaysOn>true</alwaysOn>
        <updateRate>40.0</updateRate>
        <topicName>/v1/LeftLegHokuyo/scan</topicName>
        <frameName>/v1/LeftLegHokuyo_Frame</frameName>
      </plugin>
    </sensor>
  </gazebo>
