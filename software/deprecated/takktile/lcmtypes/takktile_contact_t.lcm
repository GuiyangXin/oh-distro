package takktile;

// Message sent when there is a detection on one of the sensor channels
// detection means significant deviation from the baseline
// message fields simply indicate which sesnsors (by ID) have a detection
// and a value indicating how much the deviation is from the baseline
struct contact_t
{
  int64_t utime;

  int16_t data_length;

  int16_t id[data_length];
  int16_t delta[data_length];
}
