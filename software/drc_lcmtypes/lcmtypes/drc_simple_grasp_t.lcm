package drc;

// Matches ROS's simple grasps - for low bandwidth open-closed- grasping
struct simple_grasp_t
{
    int8_t left_state;
    int8_t right_state;

    const int8_t UNCHANGED = 0;  // leave hand in current state
    const int8_t CLOSED    = 1;  // command hand to close
    const int8_t OPEN      = 2;  // command hand to open
}
