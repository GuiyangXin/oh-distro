package drc;

struct affordance_index_t  //Associates an ee with a dof_name
{
    int64_t utime;  
    string aff_type;   // name of the otdf file
    int32_t aff_uid;   // affordance uid
    int32_t num_ees;
    string ee_name[num_ees];
    string dof_name[num_ees];   // joint name in the otdf file. 
    double dof_value[num_ees];  // associated dof_value
}

