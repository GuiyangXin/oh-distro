package drc;

struct actuator_cmd_t
{
    int64_t utime;
    string robot_name;
    int32_t num_actuators;
    string actuator_name [num_actuators];
    double actuator_effort [num_actuators];
    double effort_duration [num_actuators]; //All actuation commands are only active for a finite duration (must be set to the controller update rate or higher).
}
