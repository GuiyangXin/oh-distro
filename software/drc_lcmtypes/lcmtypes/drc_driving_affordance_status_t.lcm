package drc;

//specifies the manipulation status for a driving affordance
struct driving_affordance_status_t 
{
    int64_t utime;  
    string aff_type;   // name of the otdf file
    int32_t aff_uid;   // affordance uid
    string dof_name;
    string ee_name;

    int8_t have_manip_map; 

    double dof_value_0; // initial dof value in the manip map
    double dof_value_1; // final dof value in the manip map
}

