package drc;

struct robot_plan_constraint_checked_t
{
    int64_t utime;
    robot_plan_t robot_plan;
    
    // Additional fields for constraint checking
    const int8_t NOT_OK=0, OK=1;
    int32_t num_constraints;
    int32_t num_states; // This needs to match robot_plan.num_states
    int8_t constraints_satisfied[num_constraints][num_states];
    int8_t  CoM_constraint_satisfied;
}



