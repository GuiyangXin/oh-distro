// For translating raw hand (RawHandState) msgs from ros to lcm
// Because LCM does not allow unsigned types, we do the following substitution
// uint16 -> int32
// uint32 -> int64


package drc;

struct raw_irobot_hand_t
{
  int64_t utime;

  // Todo: more signals than palm tactile
  
  // The tactile array for the palm.  In units of ADC counts.
  float palmTactile[48];

  // The tactile temperature array for the palm, in Celsius.
  float palmTactileTemp[48];
}
