package drc;

// Seek Goal with timeout.

struct seek_goal_timed_t
{
  int64_t utime;

  // Amount of time that this goal is valid for (in nsec)
  int64_t timeout;
  string robot_name;

  // what type of goal seeking
  int8_t type;

  const int8_t VISUAL_GLOBAL        = 0;
  const int8_t VISUAL_RELATIVE      = 1;
}

