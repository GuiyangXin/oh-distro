package drc;

//specifies a desired dof state
struct affordance_goal_t  
{
    int64_t utime;  
    // name of the otdf file
    string aff_type;  
    // affordance uid 
    int8_t aff_uid;   

    int8_t num_dofs;
    // joint name in the otdf file. 
    string dof_name[num_dofs];   
    // associated dof_value
    double dof_value[num_dofs];  
}

