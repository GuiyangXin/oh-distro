package drc;

// status of the robot system
struct plan_collect_t
{
  int64_t utime;


  // Plan type: left arm, right arm, both
  int8_t type;

  // duration of plan = n_plan_states*sample_period
  int32_t n_plan_samples;
  double sample_period;
  
  const int8_t LEFT_ARM       = 0;
  const int8_t RIGHT_ARM      = 1;
  const int8_t BOTH_ARMS      = 2;
}

