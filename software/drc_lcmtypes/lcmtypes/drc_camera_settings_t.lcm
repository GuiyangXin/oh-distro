package drc;

struct camera_settings_t {
  // which camera, indicated by type of data request
  data_request_t data_request;

  // image quality/compression; see enum
  int8_t quality;

  const int8_t QUALITY_LOW        = 1;
  const int8_t QUALITY_MED        = 2;
  const int8_t QUALITY_HIGH       = 3;
  const int8_t QUALITY_SUPER      = 4;
}
