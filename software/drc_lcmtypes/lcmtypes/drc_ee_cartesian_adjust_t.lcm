package drc;

// Published on COMMITTED_EE_ADJUSTMENT via a cartesian teleop interface 
struct ee_cartesian_adjust_t 
{
  // timestamp of this message
  int64_t utime;
  int8_t ee_type;
	vector_3d_t pos_delta;
	vector_3d_t rpy_delta;
	
	const int8_t LEFT_HAND   = 0;
  const int8_t RIGHT_HAND  = 1;
}

