package drc;

// INS update packet
// Used by the data fusion process to pass INS updates from the Kalman Filter to the INS state estimate
struct ins_update_packet_t
{
  int64_t utime;

  vector_3d_t dbiasGyro_b;
  vector_3d_t dbiasAcc_b;

  vector_3d_t dE_l;
  vector_3d_t dVel_l;
  vector_3d_t dPos_l;  
}
