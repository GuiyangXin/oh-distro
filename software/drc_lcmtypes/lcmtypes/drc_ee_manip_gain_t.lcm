package drc;

// Published from the viewer via slider 
// adjustment to control on COMMITTED_MANIP_GAIN
// Real adjustment of gains for cartesian error 
// feedback to reduce SSE and also to compensate 
// for unknown inertial properties of the manipuland
struct ee_manip_gain_t 
{
  int64_t utime;
  boolean is_leg;      
  int8_t ee_type;  // see constants below 
  const int8_t LEFT=0;
  const int8_t RIGHT=1;
  const int8_t BOTH=2;
     
  double Kp;
  double Kd;
}

