package drc;

struct map_octree_t
{
  // timestamp of this message
  int64_t utime;

  // id of map
  int64_t map_id;

  // id of view
  int64_t view_id;

  // size of data
  int32_t num_bytes;

  // raw data
  byte data[num_bytes];

  // transform from map to reference coords
  transform_t transform;
}
