group "processes" {
    cmd "model_LR_RR" {
        exec = "robot_model_publisher -u $DRC_BASE/software/models/mit_gazebo_models/mit_robot/model_LR_RR.urdf";
        host = "localhost";
    }
    cmd "param_server" {
        exec = "bot-param-server drc_robot_05.cfg";
        host = "localhost";
    }
    cmd "viewer" {
        exec = "drc-viewer";
        host = "localhost";
    }
    cmd "j2f" {
        exec = "drc-joints2frames";
        host = "localhost";
    }
    cmd "tag_detector" {
        exec = "drc-hand-camera-tag -v";
        host = "localhost";
    }
}
group "planning" {
    cmd "ee-driver" {
        exec = "ee-driver";
        host = "localhost";
    }
    cmd "ReachingPlanner" {
       exec = "matlab -nosplash -nodesktop -r \"addpath_control; cd([getenv('DRC_PATH'),'/control/matlab/planners/atlas_keyframe_planners']); runKeyFramePlannersLCMBase(2);\"";
       host = "localhost";
       stop_signal = 15;
    }
}
group "logs" {
    cmd "log1" {
        exec = "lcm-logplayer-gui lcmlog-2014-02-24-12-32-robot -p";
        host = "localhost";
    }
    cmd "log2" {
        exec = "lcm-logplayer-gui lcmlog-2014-02-24-12-42-robot -p";
        host = "localhost";
    }
    cmd "log3" {
        exec = "lcm-logplayer-gui lcmlog-2014-02-24-12-44-robot -p";
        host = "localhost";
    }
}
