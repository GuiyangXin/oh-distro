group "network-test" {
    cmd "robot-netshaper" {
        exec = "drc-network-shaper -t manipulation -r robot -c subsystems/network/network-test.cfg -v";
        host = "robot";
    }
    cmd "base-netshaper" {
        exec = "drc-network-shaper -t manipulation -r base -c subsystems/network/network-test.cfg -v";
        host = "base";
    }   
    cmd "nettest" {
        exec = "drc-network-test";
        host = "robot";
    }   
}


group "spy" {
    cmd "base_spy" {
        exec = "bot-spy -l 'udpm://239.255.76.68:7668?ttl=0'";
        host = "base";
    }
    cmd "bot_spy" {
        exec = "bot-spy -l 'udpm://239.255.76.67:7667?ttl=0'";
        host = "robot";
    }
    cmd "extra_logo" {
        exec = "xlogo";
        host = "extra";
    }
    cmd "robot_yes" {
        exec = "bash -c 'while [ 1 ]; do echo 0123456789; sleep 0.001; done'";
        host = "robot";
    }
}

group "matlab" {
    cmd "matlab1" {
        exec = "matlab";
        host = "base";
    }
}