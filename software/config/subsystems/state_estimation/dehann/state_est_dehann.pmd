group "proc" {
    cmd "state-estimate" {
        exec = "state_estimate";
        host = "localhost";
    }
    cmd "MatlabEKF" {
        exec = "matlab -nosplash -nodesktop -r \"setup_datafusion; dataFusionHandler;\"";
        host = "localhost";
        stop_signal = 15;
    }
    cmd "model_LH_RH" {
        exec = "robot_model_publisher -u $DRC_BASE/software/models/mit_gazebo_models/mit_robot/model_LH_RH.urdf";
        host = "localhost";
    }
    cmd "pserver" {
        exec = "bot-param-server $DRC_BASE/software/config/drc_robot_02.cfg";
        host = "localhost";
    }
    cmd "viewer-plus" {
        exec = "drc-viewer -a";
        host = "localhost";
    }
    cmd "frame-repub" {
        exec = "se-convert-bdi-into-vicon-frame";
        host = "localhost";
    }
}
