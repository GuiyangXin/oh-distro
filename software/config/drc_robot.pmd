######################################
script "comp.qual_1" {
    start group "a.1.core";
    start group "b.1.core";
    start group "a.2.walking";
    start group "b.2.walking";
    wait ms 10000;
    start cmd "gz_quals_1"; 
    wait ms 3000;
    start cmd "set-sim-rate";
}

script "comp.qual_2" {
    start group "a.1.core";
    start group "b.1.core";
    start group "a.3.grasping";
    start group "b.3.grasping";
    wait ms 10000;
    start cmd "gz_quals_2";    
    wait ms 5000;
}

script "comp.qual_3" {
    start group "a.1.core";
    start group "b.1.core";
    start group "a.2.walking";
    start group "b.2.walking";
    wait ms 10000;
    start cmd "gz_quals_3"; 
    wait ms 3000;
    start cmd "set-sim-rate";
}

script "comp.vrc_driving" {
    start group "a.1.core";
    start group "b.1.core";
    start group "a.4.driving";
    wait ms 500;
    start cmd "gz_mit_driving";
    wait ms 6000;
    start group "b.4.driving";
}

script "comp.ingress" {
    start group "a.1.core";
    start group "b.1.core";
    start group "a.6.ingress";
    wait ms 10000;
    start cmd "gz_mit_ingress_slow"; 
    wait ms 3000;
    start cmd "set-sim-rate";
}

