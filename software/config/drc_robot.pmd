############ System Tests ############

script "systest-main" {
    run_script "dev.mit_walking";
    start cmd "systemwide-test";
    wait ms 200000;
    stop cmd "systemwide-test";
    stop group "0.development";
    stop group "a.1.core";
    stop group "a.2.walking";
    stop group "b.2.walking";
    stop cmd "BotWalkingPlanner";
    stop cmd "gz_mit_walking";
}

