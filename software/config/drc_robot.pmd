############ Misc incl System Tests ############
script "systest-core-stop" {
    stop cmd "lcm-logger-no-vis";
    stop cmd "systemwide-test";
    stop group "0.development";
    stop group "a.1.core";
    stop cmd "Qual1StateMachine";
    stop cmd "FootstepPlanner";
    stop cmd "ZMPWalkingPlanner";
    stop cmd "gz_quals_1_unpaused";
    stop cmd "set-sim-rate";
}

script "systest-main" {
    start cmd "lcm-logger-no-vis";
    start cmd "systemwide-test";
    wait ms 100;
    run_script "qual_1";
    wait ms 1200000;
    run_script "core-stop";
    wait ms 10000;
}

script "systest-vo-start" {
    start group "0.development";
    start group "a.1.core";
    stop cmd "estpub";
    start group "a.5.a.motion_vo";
    start group "a.2.walking";
    start group "b.2.walking";
    wait ms 10000;
    start cmd "gz_mit_walking_slow";
    wait ms 3000;
    start cmd "set-sim-rate-slower";
    start cmd "systemwide-test";
    start cmd "lcm-logger";
}
