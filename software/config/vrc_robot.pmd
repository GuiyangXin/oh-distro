group "m.driving_utils" {
    cmd "enable_gravity_when_pinned" {
        exec = "rostopic pub --once atlas/mode std_msgs/String 'pinned_with_gravity'";
        host = "robot";
    }
    cmd "exit_car" {
        exec = "rostopic pub --once /drc_world/robot_exit_car geometry_msgs/Pose '{}'";
        host = "robot";
    }
   cmd "enter_car_pelvis_pinned" {
        exec = "rostopic pub --once /drc_world/robot_enter_car geometry_msgs/Pose '{ position: { x: 0.0125, y: 0, z: -0.20}, orientation: { x: 0, y: 0, z: 0, w: 1 } }'";
        host = "robot";
    }  
    cmd "enable_user_mode" {
        exec = "rostopic pub --once atlas/control_mode std_msgs/String 'User'";
        host = "robot";
    }  
    cmd "unpin" {
        exec = "rostopic pub --once atlas/mode std_msgs/String 'nominal'";
        host = "robot";
    }
    
    cmd "pin" {
        exec = "rostopic pub --once atlas/mode std_msgs/String 'pinned'";
        host = "robot";
    }
    cmd "disengage_hand_brake" {
        exec = "rostopic pub --once /drc_vehicle/hand_brake/cmd std_msgs/Float64 '{ data : 0 }'";
        host = "robot";
    }
    cmd "engage_hand_brake" {
        exec = "rostopic pub --once /drc_vehicle/hand_brake/cmd std_msgs/Float64 '{ data : 1 }'";
        host = "robot";
    }
    cmd "ignition" {
        exec = "rostopic pub --once /drc_vehicle/key/cmd std_msgs/Int8 '{ data : 1 }'";
        host = "robot";
    }
    cmd "set_gear_N" {
        exec = "rostopic pub --once /drc_vehicle/direction/cmd std_msgs/Int8 '{ data : 0 }'";
        host = "robot";
    }
    cmd "set_gear_F" {
        exec = "rostopic pub --once /drc_vehicle/direction/cmd std_msgs/Int8 '{ data : 1 }'";
        host = "robot";
    }
    cmd "set_gear_R" {
        exec = "rostopic pub --once /drc_vehicle/direction/cmd std_msgs/Int8 '{ data : -1 }'";
        host = "robot";
    }
}

