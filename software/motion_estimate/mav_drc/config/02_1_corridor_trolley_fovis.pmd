group "0.development" {
    cmd "pserver" {
        exec = "bot-param-server 02_1_corridor_trolley_fovis.cfg";
        host = "localhost";
    }
    cmd "octomap" {
        exec = "octomap-server  /media/passport1/data/other/sensor_rig/octomaps/stairarea/icra_octomap/icra_octomap.bt";
        host = "localhost";
    }
    cmd "logplayer" {
        exec ="lcm-logplayer-gui /media/passport1/data/other/sensor_rig/2012-10-31-rig-stairarea-trolley/lcmlog-2012-10-31.02 -p";
        host = "localhost";
    }
    cmd "kodo" {
        exec = "kinect-odometry -pPOSE  -o KINECT_REL_ODOMETRY -u DOG_TO_CAT";
        host = "localhost";
    }

    cmd "est" {
        exec = "mav-kmcl   -P ../config/02_1_corridor_trolley_fovis.cfg   -t 0";
        host = "localhost";
    }
    cmd "vis" {
        exec = "kmcl-viewer";
        host = "localhost";
    }
    cmd "glview" {
        exec = "kinect-glview";
        host = "localhost";
    }
}
group "0.other_logs" {
    cmd "logplayer_00_works_fine_but_short" {
        exec ="lcm-logplayer-gui /media/passport1/data/other/sensor_rig/2012-10-31-rig-stairarea-trolley/lcmlog-2012-10-31.00 -p";
        host = "localhost";
    }
    cmd "logplayer_01_long_but_not_fully_stationary_at_start" {
        exec ="lcm-logplayer-gui /media/passport1/data/other/sensor_rig/2012-10-31-rig-stairarea-trolley/lcmlog-2012-10-31.01 -p";
        host = "localhost";
    }
    cmd "logplayer_02_short_and_not_at_all_stationary_at_start" {
        exec ="lcm-logplayer-gui /media/passport1/data/other/sensor_rig/2012-10-31-rig-stairarea-trolley/lcmlog-2012-10-31.02 -p";
        host = "localhost";
    }
}
script "run" { 
    start cmd "pserver";
    start cmd "vis";
    start cmd "logplayer";
    start cmd "kodo";
    start cmd "glview";
    wait ms 2000;
    start cmd "octomap";
    wait ms 2000;
    start cmd "est";
}

