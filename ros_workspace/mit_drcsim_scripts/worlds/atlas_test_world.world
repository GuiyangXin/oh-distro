<?xml version="1.0" ?>
<gazebo version="1.2">
  <world name="default">
    <gui>
    <camera name="user_camera">
      <pose>-20 10.0 2.00 0 0.2 0</pose>
    </camera>
    </gui>

    <physics type="ode">
      <gravity>0 0 -9.81</gravity>
      <!-- mfallon added -->
      <update_rate>1000</update_rate> 
      <ode>
        <solver>
          <type>quick</type>
          <dt>0.001</dt>
          <iters>40</iters>
          <sor>1.0</sor>
        </solver>
        <constraints>
          <cfm>0.0</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>100.0</contact_max_correcting_vel>
          <contact_surface_layer>0.0</contact_surface_layer>
        </constraints>
      </ode>
    </physics>
    <!-- Basic environment -->
    <include><uri>model://sun</uri></include>
    <include><uri>model://ground_plane</uri></include>

    <!-- 4 surrounding cubes -->
    <include><name>sbox1</name><uri>model://simple_box</uri><pose>5 0 1 0 0 0</pose></include>
    <include><name>sbox2</name><uri>model://simple_box</uri><pose>0 5 1 0 0 0</pose></include>
    <include><name>sbox3</name><uri>model://simple_box</uri><pose>0 -5 1 0 0 0</pose></include>
    <include><name>sbox4</name><uri>model://simple_box</uri><pose>-8 0 1 0 0 0</pose></include>

    <!-- 2 rows of boxes leading to the right of the robot -->
    <include><name>sbox_l2</name><uri>model://simple_box</uri><pose>0 -10 1 0 0 0</pose></include>
    <include><name>sbox_l3</name><uri>model://simple_box</uri><pose>0 -15 1 0 0 0</pose></include>
    <include><name>sbox_l4</name><uri>model://simple_box</uri><pose>0 -20 1 0 0 0</pose></include>
<!--    <include><name>sbox_r2</name><uri>model://simple_box</uri><pose>-8 -2.0 1 0 0 0</pose></include>
    <include><name>sbox_r3</name><uri>model://simple_box</uri><pose>-8 -7.0 1 0 0 0</pose></include> -->
    <include><name>sbox_r4</name><uri>model://simple_box</uri><pose>-8 -12.0 1 0 0 0</pose></include>
    <include><name>sbox_r5</name><uri>model://simple_box</uri><pose>-8 -17.0 1 0 0 0</pose></include>

    <!-- green and yellow path -->
    <include><name>diybox</name><uri>model://diy_box</uri><pose>7.5 -20.75 0 0 0 3.142</pose></include>

<!--    <include><name>simple_plane</name><uri>model://simple_plane</uri><pose>47.5 60.75 0 0 0 3.142</pose></include> -->

    <!-- 2 rows of boxes to the front of the robot - retained for now -->
    <include><name>simple_box_l2</name><uri>model://simple_box</uri><pose>5 4 1 0 0 0</pose></include>
    <include><name>simple_box_l3</name><uri>model://simple_box</uri><pose>10 4 1 0 0 0</pose></include>
    <include><name>simple_box_l4</name><uri>model://simple_box</uri><pose>15 4 1 0 0 0</pose></include>

    <include><name>simple_box_r2</name><uri>model://simple_box</uri><pose>7 -4 1 0 0 0</pose></include>
    <include><name>simple_box_r3</name><uri>model://simple_box</uri><pose>12 -4 1 0 0 0</pose></include>
    <include><name>simple_box_r4</name><uri>model://simple_box</uri><pose>17 -4 1 0 0 0</pose></include>

    <!-- box with window on it - for visual tracking -->
    <include><name>box_type2</name><uri>model://box_type2</uri><pose>-35 -8.5 1 0 0 0</pose></include>

    <!-- Table stuff -->
    <include><uri>model://table</uri><pose>    1.5  1.8 0.0 0 0 1.571</pose></include>
    <include><uri>model://coke_can</uri><pose> 1.6  1.6 1.05 0 0 0</pose></include>
    <include><uri>model://bowl</uri><pose>     1.6  1.3 1.10 0 0 0</pose></include>
    <include><uri>model://saucepan</uri><pose> 1.6  1.8 1.10 0 0 0</pose></include>
    <include><uri>model://fire_hose</uri><pose>0.6  0.2 1.10 0 0 1.171</pose></include>
    <include><uri>model://standpipe</uri><pose>1.25 2.4 1.1 0 0 0</pose></include>

    <include><uri>model://steering_assembly</uri><pose>0.5 -0.1524 0.0 0.0 0. 1.571</pose></include>

    <!-- DRC specific components -->
    <include><uri>model://mit_robot_PnC</uri><pose>0.0 0 1 0 0 0</pose></include>
<!--    <include><uri>model://mit_robot</uri><pose>-1.0 0 1 0 0 0</pose></include> -->
    <include><uri>model://mit_golf_cart</uri><pose>-3.5 0 0.1 0 0 -1.571</pose></include>

    <plugin filename="libVRCPlugin.so" name="vrc_plugin">
      <atlas>
         <model_name>atlas</model_name>
         <pin_link>utorso</pin_link>
      </atlas>
      <drc_vehicle>
        <model_name>mit_golf_cart</model_name>
        <seat_link>chassis</seat_link>
      </drc_vehicle>
      <drc_fire_hose>
        <fire_hose_model>fire_hose</fire_hose_model>
        <coupling_link>coupling</coupling_link>
        <standpipe_model>standpipe</standpipe_model>
        <spout_link>standpipe</spout_link>
        <thread_pitch>-1000</thread_pitch>
        <coupling_relative_pose>
          1.17038e-05 -0.125623 0.35 -0.0412152 -1.57078 1.61199
        </coupling_relative_pose>
      </drc_fire_hose>
    </plugin>

  </world>
</gazebo>
