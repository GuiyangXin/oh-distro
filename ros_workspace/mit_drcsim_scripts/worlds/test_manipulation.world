<?xml version="1.0" ?>
<sdf version="1.3">
  <world name="default">
    <physics type="ode">
      <gravity>0 0 -9.81</gravity>
      <update_rate>1000</update_rate> 
      <ode>
        <solver>
          <type>quick</type>
          <dt>0.001</dt>
          <iters>40</iters>
          <sor>1.0</sor>
        </solver>
        <constraints>
          <cfm>0.0</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>100.0</contact_max_correcting_vel>
          <contact_surface_layer>0.0</contact_surface_layer>
        </constraints>
      </ode>
    </physics>
    <!-- Basic environment -->
    <include><uri>model://sun</uri></include>
    <include><uri>model://ground_plane</uri></include>


    <!-- manipulation qual -->
    <include><uri>model://mit_table</uri><pose>    1.5  1.8 0.0 0 0 1.571</pose></include>
    <include><uri>model://mit_coke_can</uri><pose> 1.3  1.8 1.02 0 0 0</pose></include>
    <include><uri>model://bowl</uri><pose>     1.7  1.3 1.03 0 0 0</pose></include>
    <include><uri>model://saucepan</uri><pose> 1.6  1.5 1.07 0 0 0</pose></include>
    <!-- Badly modelled OSRF object <include><uri>model://fire_hose</uri><pose>0.8  0.1 1.10 0 0 1.171</pose></include>  -->
    <include><uri>model://standpipe</uri><pose>1.25 2.4 1.1 0 0 0</pose></include>
    <include><uri>model://mit_cordless_drill</uri><pose>1.3 1.3 1.02 0 0 1.571</pose></include>
    <include><uri>model://mit_cabinet</uri><pose>    1.40  0.55 0.0 0 0 0</pose></include>

    <include><uri>model://steering_assembly</uri><pose>0.5 -0.1524 0.0 0.0 0. 1.571</pose></include>





    <!-- DRC specific components -->
<!-- Walking at a height    
    <include><uri>model://mit_robot_PnC</uri><pose>0.0 0 1.5 0 0 0</pose></include> -->
<!-- Manipulation in front of the table -->
    <include><uri>model://mit_robot_PnC</uri><pose>0.7 1.50 0.95 0 0 0</pose></include>

    <include><uri>model://drc_vehicle</uri><pose>0.0 -2.55 0.1 0 0 3.142</pose></include>
<!--    <include><uri>model://golf_cart</uri><pose>-3.5 0 0.1 0 0 -1.571</pose></include> -->

    <plugin filename="libVRCPlugin.so" name="vrc_plugin">
      <atlas>
         <model_name>atlas</model_name>
         <pin_link>utorso</pin_link>
      </atlas>
      <drc_vehicle>
        <model_name>drc_vehicle</model_name>
        <seat_link>polaris_ranger_ev::chassis</seat_link>
      </drc_vehicle>
      <drc_fire_hose>
        <fire_hose_model>fire_hose</fire_hose_model>
        <coupling_link>coupling</coupling_link>
        <standpipe_model>standpipe</standpipe_model>
        <spout_link>standpipe</spout_link>
        <thread_pitch>-1000</thread_pitch>
        <coupling_relative_pose>
          1.17038e-05 -0.125623 0.35 -0.0412152 -1.57078 1.61199
        </coupling_relative_pose>
      </drc_fire_hose>
    </plugin>

  </world>
</sdf>
