<!-- joint to irobot hand -->
  <joint name="${side}_hook_hand_joint" type="fixed">
    <insert_block name="origin"/>
    <parent link="${parent}"/>
    <child link="${side}_base_link"/>
  </joint>

  <link name="${side}_base_link">
      <inertial>
        <mass value="0.1"/>
        <origin xyz="0 0 0"/>
        <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
      </inertial>

<!-- MIT's own extension plate -->
    <collision>
      <geometry>
        <cylinder length="0.01" radius="0.045"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.005"/>
    </collision>
    <visual>
      <geometry>
        <cylinder length="0.01" radius="0.045"/>
      </geometry>
      <material name="darkgray">
        <color rgba="0.2 0.2 0.2 1"/>
      </material>
      <origin rpy="0 0 0" xyz="0 0 0.005"/>
    </visual>

<!-- MIT's pointer rod -->
    <collision>
      <geometry>
        <cylinder length="0.16" radius="0.006"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.08"/>
    </collision>
    <visual>
      <geometry>
        <cylinder length="0.16" radius="0.006"/>
      </geometry>
      <material name="darkgray">
        <color rgba="0.2 0.2 0.2 1"/>
      </material>
      <origin rpy="0 0 0" xyz="0 0 0.08"/>
    </visual>


<!-- MIT's hook tip -->
